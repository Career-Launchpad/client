  "scripts": {
    "start": "env-cmd -f ./.env.build react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "prettier --check \"**/*.+(js|jsx|json|css|md)\"",
    "lint:fix": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "update-schema": "get-graphql-schema https://0379pmxh99.execute-api.us-east-1.amazonaws.com/dev/graphql > src/graphql/schema.graphql",
    "update-schema-local": "get-graphql-schema http://127.0.0.1:8080 > src/graphql/schema.graphql",
    "relay": "npm run update-schema && npm run relay-compile",
    "relay-local": "npm run update-schema-local && npm run relay-compile",
    "relay-compile": "relay-compiler --watch --config relay.config.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^14.5.8",
    "prettier": "^1.19.1",
    "relay-compiler": "^8.0.0",
    "relay-config": "^7.1.0"
  }
