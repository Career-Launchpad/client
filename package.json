  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "aws-amplify": "^1.2.4",
    "babel-plugin-relay": "^7.1.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-beta.5",
    "farce": "^0.2.8",
    "formik": "^2.0.5",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-number-format": "^4.3.1",
    "react-relay": "^7.1.0",
    "react-relay-network-modern": "^4.3.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "uuid": "^3.3.3",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start --no-cache",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "update-schema": "get-graphql-schema https://0379pmxh99.execute-api.us-east-1.amazonaws.com/dev/graphql > src/graphql/schema.graphql",
    "update-schema-local": "get-graphql-schema http://127.0.0.1:8080 > src/graphql/schema.graphql",
    "relay": "npm run update-schema && relay-compiler --config relay.config.json",
    "relay-local": "npm run update-schema-local && relay-compiler --config relay.config.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run relay && pretty-quick --staged && git add ."
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "graphql": "^14.5.8",
    "husky": "^3.0.9",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "relay-compiler": "^8.0.0",
    "relay-config": "^7.1.0"
  }
}
